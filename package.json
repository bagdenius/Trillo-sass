{
  "name": "trillo-sass",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "compile-sass": "sass sass/main.scss css/main.compiled.css",
    "concat-css": "concat -o css/main.concat.css css/icon-font.css css/main.compiled.css",
    "prefix-css": "postcss --use autoprefixer -b 'last 10 versions' css/main.concat.css -o css/main.prefixed.css",
    "compress-css": "sass css/main.prefixed.css css/main.css --style compressed",
    "build": "npm-run-all compile-sass concat-css prefix-css compress-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bagdenius/Trillo-sass.git"
  },
  "author": "bagdenius",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bagdenius/Trillo-sass/issues"
  },
  "homepage": "https://github.com/bagdenius/Trillo-sass#readme",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "sass": "^1.92.0"
  }
}
